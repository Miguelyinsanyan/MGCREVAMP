local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("MGC") -- Creates the window

local b = w:CreateFolder("Kagune") -- Creates the folder(U will put here your buttons,etc)
local c = w:CreateFolder("Quinque")
local d = w:CreateFolder("Credits")

b:Label("Kagune",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
   

}) 

b:Button("Eto",function()
    local args = {
        [1] = "Eto", -- change to kagunes/quinques you want
        [2] = "Slot1"
     }
     
     game:GetService("ReplicatedStorage").Remotes.Slotfunction:InvokeServer(unpack(args))
end)

b:Button("Tatara",function()
